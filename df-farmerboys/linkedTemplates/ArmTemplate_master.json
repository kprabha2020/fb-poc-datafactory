{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "df-farmerboys"
		},
		"LS_AZ_SQL_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'LS_AZ_SQL'"
		},
		"LS_BlobStorageAccount_Generic_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'LS_BlobStorageAccount_Generic'"
		},
		"LS_Config_DB_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'LS_Config_DB'"
		},
		"AZ_KeyVault_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://fbazkeyvault.vault.azure.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "df-farmerboys_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"LS_AZ_SQL_connectionString": {
						"value": "[parameters('LS_AZ_SQL_connectionString')]"
					},
					"LS_BlobStorageAccount_Generic_connectionString": {
						"value": "[parameters('LS_BlobStorageAccount_Generic_connectionString')]"
					},
					"LS_Config_DB_connectionString": {
						"value": "[parameters('LS_Config_DB_connectionString')]"
					},
					"AZ_KeyVault_properties_typeProperties_baseUrl": {
						"value": "[parameters('AZ_KeyVault_properties_typeProperties_baseUrl')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}